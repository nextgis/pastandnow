{
  "name": "@nextgis/vue-ngw-maplibre-gl",
  "version": "2.2.1",
  "description": "Vue NextGIS Web map component for Maplibre GL JS",
  "main": "lib/vue-ngw-maplibre-gl.js",
  "module": "lib/vue-ngw-maplibre-gl.esm-bundler.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@nextgis/vue2-ngw-map": "^2.2.1"
  },
  "peerDependencies": {
    "@nextgis/maplibre-gl-map-adapter": "*"
  },
  "devDependencies": {
    "@nextgis/build-tools": "^2.0.2",
    "@nextgis/maplibre-gl-map-adapter": "^3.0.0-alpha.13",
    "maplibre-gl": "^4.7.0"
  },
  "buildOptions": {
    "name": "VueNgwMaplibregl",
    "injectCss": true,
    "tsbuild": true,
    "formats": [
      "esm-bundler"
    ]
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "dev": "node ../build-tools/lib/build.js",
    "prod": "npm run dev -- --release",
    "lint": "eslint ./src/**/*.ts --fix --c ../../.eslintrc",
    "watch": "npm run dev -- --watch"
  },
  "keywords": [
    "NextGIS",
    "MAP"
  ],
  "author": "NextGIS",
  "files": [
    "index.js",
    "lib"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nextgis/nextgis_frontend_vue2/tree/master/packages/vue2-ngw-maplibre-gl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextgis/nextgis_frontend_vue2.git"
  },
  "bugs": {
    "url": "https://github.com/nextgis/nextgis_frontend_vue2/issues"
  }
}
